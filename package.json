      "aws-sdk": "^2.1693.0",
      "compression": "^1.8.1",
      "config": "^4.1.1",
      "cors": "^2.8.5",
      "cron": "^4.3.3",
      "dayjs": "^1.11.18",
      "dotenv": "^17.2.3",
      "express": "^5.1.0",
      "graceful-fs": "^4.2.11",
      "gulp": "^5.0.1",
      "lodash": "^4.17.21",
      "luxon": "^3.7.2",
      "multer": "^2.0.2",
      "nanoid": "^5.1.6",
